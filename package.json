    "ejs": "^2.3.1",
    "express": "^4.13.3",
    "express-recaptcha": "^2.2.0",
    "frontend-dependencies": "^0.2.1",
    "jquery-validation": "^1.17.0",
    "mongo-factory": "^1.0.0",
    "mongodb": "^2.2.31",
    "morgan": "^1.6.1",
    "serve-favicon": "^2.3.0",
    "stopwatch-js": "0.0.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "debug": "^2.2.0",
    "gulp": "^3.9.0",
    "gulp-nodemon": "^2.0.2",
    "gulp-livereload": "^3.8.0",
    "gulp-plumber": "^1.0.0",
    "mocha": "^3.0.2",
    "nyc": "^10.0.0",
    "supertest": "^2.0.0"
